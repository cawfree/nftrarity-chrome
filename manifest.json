{
  "manifest_version": 2,
  "name": "nftrarity-chrome",
  "version": "0.1.0",
  "description": "Displays NFT Rarity on OpenSea.",
  "content_scripts": [{
    "js": ["index.js"],
    "matches": ["http://*/*",
                "https://*/*"
    ]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
